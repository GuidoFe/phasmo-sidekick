{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-esling"
    ],
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google",
        "plugin:@typescript-eslint/recommended"
    ],
    "parserOptions": {
        "requireConfigFile": false,
        "ecmaVersion": 12
    },
    "rules": {
        "indent": ["error", 4],
        "max-len": ["off", {"code": 80, "ignoreComments": true, "ignoreUrls":true, "ignoreStrings":true}],
        "require-jsdoc": ["off"]
    }
}
