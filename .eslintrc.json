{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": ["error", 4],
        "max-len": ["warn", {"code": 80, "ignoreComments": true, "ignoreUrls":true, "ignoreStrings":true}]
    }
}
